{
    "id": "terraform-docs",
    "version": "1.0.0",
    "name": "terraform-docs",
    "description": "install latest version of terraform-docs",
    "options": {
        "version": {
            "type": "string",
            "proposals": [
                "latest",
                "none",
                "0.16.0"
            ],
            "default": "latest",
            "description": "Select or enter a terraform-docs version to install"
        },
        "sha256": {
            "type": "string",
            "proposals": [
                "automatic",
                "dev-mode"
            ],
            "default": "automatic",
            "description": "Select or enter a terraform-docs version SHA256 to check"
        }
    }
}