{
    "id": "build-tools",
    "version": "1.0.0",
    "name": "build-tools",
    "description": "install latest version of build tools",
    "options": {
        "gomplate": {
            "type": "string",
            "proposals": [
                "latest",
                "none",
                "4.28.1"
            ],
            "default": "latest",
            "description": "Select or enter a yq version to install"
        },
        "yq_sha256": {
            "type": "string",
            "proposals": [
                "automatic",
                "dev-mode"
            ],
            "default": "automatic",
            "description": "Select or enter a yq version SHA256 to check"
        }
    }
}