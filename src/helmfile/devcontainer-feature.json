{
    "id": "helmfile",
    "version": "1.0.0",
    "name": "Helmfile",
    "description": "Installs latest version of helmfile. Auto-detects latest versions and installs needed dependencies.",
    "options": {
        "version": {
            "type": "string",
            "proposals": [
                "latest",
                "none",
                "v0.146.0",
                "v0.145.5",
                "v0.145.4",
                "v0.145.3",
                "v0.145.2",
                "v0.145.1",
                "v0.145.0"
            ],
            "default": "latest",
            "description": "Select or enter a helmfile version to install"
        }
    }
}
